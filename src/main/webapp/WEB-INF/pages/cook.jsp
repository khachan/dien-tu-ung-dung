			<script>
	var numberOfProductsInRow = -1;

	function updateRows(fromIndex){
		return;
		if (numberOfProductsInRow < 0) return;
		var isIE = '-ms-scroll-limit' in document.documentElement.style && '-ms-ime-align' in document.documentElement.style;
		var start = fromIndex ? fromIndex : 0; 
		var rowHeight = 0;
		var cellHeight = 0;
		var items = $('.product-grid-item');
		for (var i = start; i<items.length; i++){
			$(items[i]).removeAttr('style');
			if (isIE){
				$(items[i]).find('img').removeAttr('style');
			}
		}
		for (var i = start; i<items.length; i++){
			// Start of row
			if (i % numberOfProductsInRow == 0){
				rowHeight = 0;
				cellHeight = 0;
				for (var j=i;j<i+numberOfProductsInRow && j<items.length;j++){
					var height = $(items[j]).height();
					if (height > rowHeight) rowHeight = height;
					var tempCell = $(items[j]).find('table').height;
					if (cellHeight > 0 && cellHeight > tempCell) cellHeight = tempCell;
				}
			}
			$(items[i]).height(rowHeight);
			if (isIE){
				$(items[i]).find('img').attr('style', 'max-height: '+rowHeight+'px;');
			}
		}
	}
	$('.product-grid').imagesLoaded(function(){
		// Single Column
		enquire.register("screen and (max-width:767px)", {
			match : function() {
				numberOfProductsInRow = 2;
				updateRows();
			},
		});
		// Two Columns
		enquire.register("screen and (min-width: 768px) and (max-width: 991px)", {
			match : function() {
				numberOfProductsInRow = 4;
				updateRows();
			},
		});
		// Four Columns
		enquire.register("screen and (min-width: 992px) and (max-width: 1199px)", {
			match : function() {
				numberOfProductsInRow = 4;
				updateRows();
			},
		});
		// Five Columns
		enquire.register("screen and (min-width: 1200px)", {
			match : function() {
				numberOfProductsInRow = 5;
				updateRows();
			},
		});
